{
  "author": "Nicolas Bourasseau",
  "bugs": {
    "url": "https://github.com/Imbuzi/BouBouBox/issues"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "sqlite3": "^3.1.13",
    "express": "^4.15.4",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.0.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "mysql": "^2.14.1",
    "node-milight-promise": "^0.2.32",
    "nodemon": "^1.12.1",
    "nouislider": "^10.1.0",
    "password-hash": "^1.2.2",
    "path": "^0.12.7",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "vue": "^2.4.4",
    "vue-localstorage": "^0.4.2",
    "vue-lodash": "^1.0.4",
    "vue-router": "^2.7.0",
    "vue-sessionstorage": "^1.0.0",
    "vue-socket.io": "^2.1.1-a",
    "vuex": "^2.4.0"
  },
  "description": "BouBouBox Node JS Package",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^5.0.5",
    "envify": "^4.1.0",
    "http-server": "^0.10.0",
    "npm-run-all": "^4.1.1",
    "uglify-js": "^3.1.3",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "homepage": "https://github.com/Imbuzi/BouBouBox#readme",
  "license": "ISC",
  "main": "server.js",
  "name": "bouboubox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Imbuzi/BouBouBox.git"
  },
  "scripts": {
    "watchify": "watchify -vd -e clientApp.js -o public/js/app.js",
    "nodemon": "nodemon",
    "dev": "npm-run-all --parallel watchify nodemon",
    "build": "cross-env NODE_ENV=production browserify -g envify clientApp.js | uglifyjs -c warnings=false -m > public/js/app.js",
    "build-dev": "cross-env NODE_ENV=development browserify -g envify clientApp.js | uglifyjs -c warnings=false -m > public/js/app.js",
    "db": "knex migrate:rollback && knex migrate:latest && knex seed:run"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "version": "1.0.0"
}
